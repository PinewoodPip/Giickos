<?xml version="1.0" encoding="utf-8"?>
<resources >

    <style name="GiickosInteractableCard">
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:clickable">true</item>
        <item name="android:foreground">?attr/selectableItemBackground</item>
        <item name="android:backgroundTint">@color/float_background</item>\
    </style>

    <style name="GiickosFancyCard" xmlns:app="http://schemas.android.com/apk/res-auto">
        <item name="android:clickable">true</item>
        <item name="android:foreground">?attr/selectableItemBackground</item>
        <item name="android:focusable">true</item>
        <item name="android:backgroundTint">#87837E</item>\
    </style>

    <style name="GiickosSpinner" parent="@android:style/Widget.Holo.DropDownItem.Spinner">
        <item name="android:textColor">@color/text</item>
    </style>

    <style name="GiickosBackground">
        <item name="android:backgroundTint">#51A354</item>
    </style>

    <style name="GiickosFullscreenContainer">
        <item name="android:padding">5dp</item>
    </style>

    <style name="horizontal_divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">3dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>

    <style name="a">
        <item name="android:layout_width">137dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="BottomBorder">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">5dp</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:background">#46000000</item>
    </style>

    <style name="back">
        <item name="android:layout_width">160dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="frame">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">100dp</item>
        <item name="android:background">@drawable/blank_frame</item>
        <item name="android:backgroundTint">#326504</item>
        <item name="android:backgroundTintMode">multiply</item>
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="e">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_alignParentTop">false</item>
        <item name="android:layout_alignParentEnd">false</item>
        <item name="android:visibility">invisible</item>
    </style>
</resources>